public class Test {
    static int a;
    int b;

    public static void main(String[] args) {
        Test obj1 = new Test();
        Test obj2 = new Test();

        obj1.a = 5;    
        obj1.b = 10;   
        obj2.a = 7;    
        obj2.b = 20;   

        System.out.println("Object 1 - a: " + obj1.a + ", b: " + obj1.b);
        System.out.println("Object 2 - a: " + obj2.a + ", b: " + obj2.b);
    }
}
