abstract class Bird {
    public abstract void sing();
    public void eat() {
        System.out.println("Eating...");
    }
}

class Sparrow extends Bird {
    @Override
    public void sing() {
        System.out.println("Singing...");
    }
}

public class Main {
    public static void main(String[] args) {
        Sparrow sparrow = new Sparrow();
        sparrow.sing();
        sparrow.eat();
    }
}
