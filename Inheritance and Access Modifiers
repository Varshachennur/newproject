class Animal {
    public void eat() {
        System.out.println("Eating...");
    }

    private void sleep() {
        System.out.println("Sleeping...");
    }
}

class Dog extends Animal {
    public void makeSound() {
        eat();  
         sleep(); 
    }

    public static void main(String[] args) {
        Dog dog = new Dog();
        dog.eat();  
        dog.sleep(); 
    }
}
