abstract class Animal {
    abstract void speak();

    void eat() {
        System.out.println("Eating...");
    }
}

class Dog extends Animal {
    public void speak() {
        System.out.println("Woof!");
    }

    public static void main(String[] args) {
        Dog dog = new Dog();
        dog.speak();  // Output: Woof!
        dog.eat();    // Output: Eating...
    }
}
