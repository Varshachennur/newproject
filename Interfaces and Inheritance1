class Animal {
}

class Dog extends Animal {
}

class Cat extends Animal {
}

public class Main {
    public static void main(String[] args) {
        Dog dog = new Dog();
        Cat cat = new Cat();

        System.out.println("Is dog an instance of Animal? " + (dog instanceof Animal));
        System.out.println("Is cat an instance of Animal? " + (cat instanceof Animal));
    }
}
